{
	"name": "react-boilerplate-client",
	"version": "1.0.0",
	"description": "boilerplate for front-end react project, contains: react, react-router, webpack, sass",
	"main": "app.js",
	"scripts": {
		"start": "npm run dev-server",
		"dev:server": "start http://localhost:8080/ & webpack-dev-server --config ./webpack.config.js --mode development",
		"build": "webpack --mode production"
	},
	"author": "steve failla",
	"license": "ISC",
	"dependencies": {
		"node-sass": "^4.12.0",
		"normalize.css": "^8.0.1",
		"react": "^16.10.2",
		"react-dom": "^16.10.2",
		"react-router-dom": "^5.1.2",
		"webpack-dev-server": "^3.8.2"
	},
	"devDependencies": {
		"@babel/core": "^7.6.4",
		"@babel/plugin-proposal-class-properties": "^7.5.5",
		"@babel/plugin-proposal-object-rest-spread": "^7.6.2",
		"@babel/plugin-transform-runtime": "^7.6.2",
		"@babel/preset-env": "^7.1.6",
		"@babel/preset-es2017": "^7.0.0-beta.53",
		"@babel/preset-react": "^7.0.0",
		"babel-cli": "^6.26.0",
		"babel-loader": "^8.0.6",
		"css-loader": "^3.2.0",
		"file-loader": "^4.2.0",
		"image-webpack-loader": "^6.0.0",
		"mini-css-extract-plugin": "^0.8.0",
		"sass-loader": "^8.0.0",
		"style-loader": "^1.0.0",
		"webpack": "^4.25.1",
		"webpack-cli": "^3.1.2"
	}
